#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/pino@10.1.0/node_modules/pino/node_modules:/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/pino@10.1.0/node_modules:/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/pino@10.1.0/node_modules/pino/node_modules:/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/pino@10.1.0/node_modules:/mnt/c/Users/hrg53/OneDrive/Desktop/ITMBU-CANTEEN/app/offline-ordering/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pino/bin.js" "$@"
else
  exec node  "$basedir/../pino/bin.js" "$@"
fi
